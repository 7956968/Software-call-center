  <log4net>
    <appender name="ConsoleAppender" type="log4net.Appender.ConsoleAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%m%n" />
      </layout>
    </appender>
    <appender name="AuditAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <file value="Logs/redfox_controller_audit.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="\.yyyy-MM-dd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level %env %message%newline" />
      </layout>
    </appender>
    <appender name="SystemAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <file value="Logs/redfox_controller_system.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="\.yyyy-MM-dd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level %message%newline" />
      </layout>
    </appender>
    <appender name="SessionAppender" type="log4net.Appender.RollingFileAppender,log4net">
      <file value="Logs/redfox_controller_session.log" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <datePattern value="\.yyyy-MM-dd" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date %-5level %message%newline" />
      </layout>
    </appender>
    <logger name="Session">
      <level value="Debug" />
      <appender-ref ref="SessionAppender" />
    </logger>
    <logger name="Audit">
      <level value="Debug" />
      <appender-ref ref="AuditAppender" />
    </logger>
    <logger name="System">
      <level value="All" />
      <appender-ref ref="SystemAppender" />
    </logger>
    <root>
      <priority value="Debug" />
      <appender-ref ref="ConsoleAppender" />
    </root>
  </log4net>
